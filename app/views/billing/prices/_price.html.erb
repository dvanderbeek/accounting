<div id="<%= dom_id price %>">
  <p>
    <strong>Product:</strong>
    <%= price.product_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= price.name %>
  </p>

  <p>
    <strong>Billing scheme:</strong>
    <%= price.billing_scheme %>
  </p>

  <p>
    <strong>Price per unit cents:</strong>
    <%= price.price_per_unit_cents %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= price.currency %>
  </p>

</div>
